<?xml version="1.0" encoding="UTF-8"?>
<openbox_menu>
<menu id="root-menu" label="root-menu">
  <menu id="Accessories" label="Accessories">
    <item label="File Roller">
      <action name="Execute"><command>file-roller</command></action>
    </item>
    <item label="Nemo">
      <action name="Execute"><command>nemo</command></action>
    </item>
  </menu> <!-- Accessories -->
  <menu id="Development" label="Development">
    <item label="Electron 34">
      <action name="Execute"><command>electron34</command></action>
    </item>
  </menu> <!-- Development -->
  <menu id="Graphics" label="Graphics">
    <item label="Document Viewer">
      <action name="Execute"><command>evince</command></action>
    </item>
  </menu> <!-- Graphics -->
  <menu id="Internet" label="Internet">
    <item label="Avahi SSH Server Browser">
      <action name="Execute"><command>/usr/bin/bssh</command></action>
    </item>
    <item label="Avahi VNC Server Browser">
      <action name="Execute"><command>/usr/bin/bvnc</command></action>
    </item>
    <item label="Vesktop">
      <action name="Execute"><command>vesktop</command></action>
    </item>
    <item label="WARP Off">
      <action name="Execute"><command>/home/sunny/.local/share/warp/warp.off.sh</command></action>
    </item>
    <item label="WARP On">
      <action name="Execute"><command>/home/sunny/.local/share/warp/warp.on.sh</command></action>
    </item>
    <item label="Zen Browser">
      <action name="Execute"><command>/opt/zen-browser-bin/zen-bin</command></action>
    </item>
  </menu> <!-- Internet -->
  <menu id="Office" label="Office">
    <item label="Document Viewer">
      <action name="Execute"><command>evince</command></action>
    </item>
  </menu> <!-- Office -->
  <menu id="Settings" label="Settings">
    <item label="GTK Settings">
      <action name="Execute"><command>nwg-look</command></action>
    </item>
  </menu> <!-- Settings -->
  <menu id="System" label="System">
    <item label="Alacritty">
      <action name="Execute"><command>alacritty</command></action>
    </item>
    <item label="Avahi Zeroconf Browser">
      <action name="Execute"><command>/usr/bin/avahi-discover</command></action>
    </item>
    <item label="fish">
      <action name="Execute"><command>fish</command></action>
    </item>
  </menu> <!-- System -->
  <menu id="Other" label="Other">
    <item label="Cloudflare Zero Trust">
      <action name="Execute"><command>systemctl --user start warp-taskbar</command></action>
    </item>
  </menu> <!-- Other -->
</menu> <!-- root-menu -->
</openbox_menu>
